#include <iostream>
#include<bits/stdc++.h>

using namespace std;
int main() {
    int t;
    cin>>t;
    vector<pair<int,int>>v;
    for(int i=0;i<t;i++)
    {
        int a,b;
        cin>>a>>b;
        v.push_back(make_pair(a,b));
    }
    sort(v.begin(),v.end());
    for(int i=0;i<t;i++)
    {
        cout<<v[i].first<<" "<<v[i].second<<"\n";
    }
}
