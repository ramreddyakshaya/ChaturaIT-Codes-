#include <bits/stdc++.h>
 
#define int long long
#define nn '\n'
typedef long long ll;
 
using namespace std;
 
int32_t main()
{
        ios_base::sync_with_stdio(false);
        cin.tie(0);
        cout.tie(0);
 
        int n;
        cin>>n;
        int ans=0;
        for(int i=0;i<n;i++)
        {
            int x;
            cin>>x;
            ans+=x+(x-1)*i;
        }cout<<ans;
}

//https://codeforces.com/contest/103/problem/A
