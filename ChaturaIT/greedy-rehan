#include <iostream>

using namespace std;

int main() {
    long long int A, B, K;
    cin >> A;
    cin >> B;
    cin >> K;

    for (long long int i = 0; i < K; i++) {
        if (A > 0) {
            A--;
        } else if (B > 0) {
            B--;
        }
    }   cout<< A << " "<< B<< endl;

    return 0;
}
