#include<bits/stdc++.h>
using namespace std;
 
 
int main(){
    int a,b,c,d;
    int sum=0;
    scanf("%d%d%d%d",&a,&b,&c,&d);//2 3 5 6
    int cnt=min(a,min(c,d));
    for(int i=0;i<cnt;i++){
        sum+=526;
    }
    for(int i=0;i<min(a-cnt,b);i++){
        sum+=23;
    }
    printf("%d",sum);
    return 0;
}

//Reference:https://codeforces.com/contest/734/problem/B
